@using Feeder4.Dtos;
@model UserRegisterDto

@{
    ViewData["Title"] = "RegisterUserPage";
}

<form method="post">
    <div class="form-group">
        <label>Name</label>
        <input asp-for="Name" class="form-control" placeholder="Model">
    </div> <br>
    <div class="form-group">
        <label>Email</label>
        <input asp-for="Email" class="form-control" placeholder="Size">
    </div> <br>
    <div class="form-group">
        <label>Password</label>
        <input asp-for="Password" class="form-control" placeholder="Capacity">
    </div> <br>
    <button type="submit" class="btn btn-primary">Submit</button>
</form>